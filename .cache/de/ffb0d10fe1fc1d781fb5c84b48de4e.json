ing: 0;\r\n    box-sizing: inherit;\r\n}\r\n\r\n\r\n:root {\r\n    font-size: 62.5%;\r\n    font-family: 'Open Sans', sans-serif;\r\n    line-height: 1.6;\r\n\r\n    --clr-red: #CE4E12;\r\n    --clr-red-lighten: #f3af90;\r\n    --clr-green: #2FBF71;\r\n\r\n    --clr-text-dark: #444;\r\n    --clr-text-light: #eee;\r\n}\r\nbody {\r\n    font-size: 1.2rem;\r\n    background-color: #eee;\r\n}\r\n\r\n.red {\r\n    color: var(--clr-red) !important;\r\n}\r\n.red-focus:focus {\r\n    border: 1px solid var(--clr-red) !important;\r\n}\r\n.heading-1 {\r\n    font-size: 4.5rem;\r\n}\r\n.heading-2 {\r\n    font-size:2.2rem;\r\n}\r\n","styles\\_top.scss":".top {\r\n    background-image: \r\n        linear-gradient(rgba(0, 0, 0, 0.35), rgba(0, 0, 0, 0.35)),\r\n        url(../images//back.png);\r\n    background-size: cover;\r\n    background-position: center;\r\n    display: grid;\r\n    justify-content: center;\r\n    align-content: center;\r\n    min-height: 40vh;\r\n    color: var(--clr-text-light);\r\n}\r\n\r\n.budget {\r\n\r\n    &__value {\r\n        display:grid;\r\n        justify-content: center;\r\n    }\r\n    &__income,\r\n    &__expenses {\r\n        display: flex;\r\n        justify-content: space-between;\r\n        align-items: center;\r\n        font-size: 2rem;\r\n        padding: 1rem;\r\n        margin: 1rem 0;\r\n        font-size: 1.6rem;\r\n        font-weight: 600;\r\n        \r\n        &--text {\r\n            text-transform: uppercase;\r\n            color: var(--clr-text-dark);\r\n            font-size: 1.4rem;\r\n        }\r\n        & .right {\r\n            display: grid;\r\n            grid-template-columns: repeat(2, max-content);\r\n            grid-column-gap: .5rem;\r\n        }\r\n    }\r\n    &__income {\r\n        background-color: var(--clr-green);\r\n        &--percentage {\r\n            width: 4rem;\r\n        }\r\n\r\n\r\n\r\n    }\r\n    &__expenses {\r\n        background-color: var(--clr-red);   \r\n\r\n        &--percentage {\r\n            background-color: var(--clr-red-lighten);\r\n            text-align: center;\r\n            font-size: 1.4rem;\r\n            width: 4rem;\r\n            display: flex;\r\n            align-content: flex-start;\r\n            justify-content: center;\r\n            color: var(--clr-text-dark);\r\n        }\r\n\r\n    }\r\n}","styles\\_input.scss":".add {\r\n    height: 10vh;\r\n    background-color: #dddddd;\r\n    display: grid;\r\n    align-items: center;\r\n    justify-items: center;\r\n    font-size: 1.6rem;\r\n\r\n    &__container {\r\n        display: flex;\r\n        align-items: center;\r\n    }\r\n\r\n    &__type,\r\n    &__description,\r\n    &__value {\r\n        border: 1px solid var(--clr-text-dark);\r\n        border-radius: .5rem;\r\n        font-size: inherit;\r\n        padding: 1rem;\r\n        margin: 0 .3rem;\r\n        font-family: inherit;\r\n        outline: none;\r\n        transition: all 300ms;\r\n    }\r\n\r\n    &__description:focus,\r\n    &__value:focus,\r\n    &__type:focus {\r\n        border: 1px solid var(--clr-green);\r\n        outline: none;\r\n    }\r\n\r\n    &__value {\r\n        width: 8rem;\r\n    }\r\n    &__description {\r\n        width: 30rem;\r\n    }\r\n\r\n    &__btn {\r\n        border: none;\r\n        font-size: 3.2rem;\r\n        color: var(--clr-text-dark);\r\n        background-color: transparent;\r\n        margin-left: .5rem;\r\n        cursor: pointer;\r\n        outline: none;\r\n        transition: all 100ms;\r\n\r\n        &:active {\r\n            transform: translateY(-2px) scale(1.01);\r\n        }\r\n\r\n        &:focus {\r\n            transform: scale(1.1);\r\n        }\r\n    }\r\n}","styles\\_lists.scss":".container {\r\n    width: 1000px;\r\n    margin: 60px auto;\r\n    display: grid;\r\n    grid-template-columns: repeat(2, 1fr);\r\n    grid-column-gap: 2rem;\r\n    \r\n    & h2 {\r\n        text-transform: uppercase;\r\n        font-size: 18px;\r\n        margin-bottom: 15px;\r\n    }\r\n}\r\n.income__title {\r\n    color: var(--clr-green);\r\n}\r\n.expenses__title {\r\n    color: var(--clr-red);\r\n}\r\n\r\n.item {\r\n    padding: 13px;\r\n    border-bottom: 1px solid #e7e7e7;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    font-size: 1.6rem;\r\n    \r\n\r\n    &:first-child {\r\n         border-top: 1px solid #e7e7e7;\r\n    }\r\n    &:nth-child(even) {\r\n         background-color: #f7f7f7;\r\n    }\r\n\r\n    & .right{\r\n        display: flex;\r\n    }\r\n    &__value {\r\n        transition: transform 0.3s;\r\n    }\r\n\r\n    &__percentage {\r\n        margin-left: 2rem;\r\n        transition: transform 0.3s;\r\n        font-size: 1.1rem;\r\n        background-color: var(--clr-red-lighten);\r\n        padding: .3rem;\r\n        border-radius: .3rem;\r\n        width: 3.2rem;\r\n        text-align: center;\r\n    }\r\n    &__delete--btn {\r\n        font-size: 22px;\r\n        background: none;\r\n        border: none;\r\n        cursor: pointer;\r\n        display: inline-block;\r\n        vertical-align: middle;\r\n        line-height: 1;\r\n        display: none;\r\n\r\n        &:focus {\r\n            outline: none;\r\n        }\r\n        &:active {\r\n            transform: translateY(2px);\r\n        }\r\n    }\r\n    &:hover &__delete--btn { display: block; } \r\n    &:hover &__value { transform: translateX(-2rem); }\r\n    &:hover &__percentage { transform: translateX(-2rem); }\r\n\r\n}\r\n.income {\r\n    & .right, \r\n    & .item__delete--btn {\r\n        color: var(--clr-green);\r\n    }\r\n}\r\n.expenses {\r\n    & .right,\r\n    & .item__delete--btn {\r\n        color: var(--clr-red);\r\n    }\r\n}\r\n\r\n.unpaid {\r\n    background-color: var(--clr-red-lighten) !important;\r\n    cursor: pointer;\r\n    color: var(--clr-red);\r\n\r\n    & .item__percentage {\r\n         box-shadow: 0 2px 6px 0 rgba(0, 0, 0, 0.1); \r\n    }\r\n\r\n    &:hover .item__description {\r\n         font-weight: 900; \r\n    }\r\n}\r\n\r\n"},"lineCount":null}},"error":null,"hash":"612d67dd6f45a77b3d8c91508d8920f7","cacheData":{"env":{}}}