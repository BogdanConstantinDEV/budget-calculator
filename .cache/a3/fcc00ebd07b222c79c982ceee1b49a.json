: var(--clr-text-dark);\r\n        }\r\n\r\n    }\r\n}","styles\\_input.scss":".add {\r\n    height: 10vh;\r\n    background-color: #dddddd;\r\n    display: grid;\r\n    align-items: center;\r\n    justify-items: center;\r\n    font-size: 1.6rem;\r\n\r\n    &__container {\r\n        display: flex;\r\n        align-items: center;\r\n    }\r\n\r\n    &__type,\r\n    &__description,\r\n    &__value {\r\n        border: 1px solid var(--clr-text-dark);\r\n        border-radius: .5rem;\r\n        font-size: inherit;\r\n        padding: 1rem;\r\n        margin: 0 .3rem;\r\n        font-family: inherit;\r\n        outline: none;\r\n        transition: all 300ms;\r\n    }\r\n\r\n    &__description:focus,\r\n    &__value:focus,\r\n    &__type:focus {\r\n        border: 1px solid var(--clr-green);\r\n        outline: none;\r\n    }\r\n\r\n    &__value {\r\n        width: 8rem;\r\n    }\r\n    &__description {\r\n        width: 30rem;\r\n    }\r\n\r\n    &__btn {\r\n        border: none;\r\n        font-size: 3.2rem;\r\n        color: var(--clr-text-dark);\r\n        background-color: transparent;\r\n        margin-left: .5rem;\r\n        cursor: pointer;\r\n        outline: none;\r\n        transition: all 100ms;\r\n\r\n        &:active {\r\n            transform: translateY(-2px) scale(1.01);\r\n        }\r\n\r\n        &:focus {\r\n            transform: scale(1.1);\r\n        }\r\n    }\r\n}","styles\\_lists.scss":".container {\r\n    width: 1000px;\r\n    margin: 60px auto;\r\n    display: grid;\r\n    grid-template-columns: repeat(2, 1fr);\r\n    grid-column-gap: 2rem;\r\n    \r\n    & h2 {\r\n        text-transform: uppercase;\r\n        font-size: 18px;\r\n        margin-bottom: 15px;\r\n    }\r\n}\r\n.income__title {\r\n    color: var(--clr-green);\r\n}\r\n.expenses__title {\r\n    color: var(--clr-red);\r\n}\r\n\r\n.item {\r\n    padding: 13px;\r\n    border-bottom: 1px solid #e7e7e7;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    font-size: 1.6rem;\r\n    \r\n\r\n    &:first-child {\r\n         border-top: 1px solid #e7e7e7;\r\n    }\r\n    &:nth-child(even) {\r\n         background-color: #f7f7f7;\r\n    }\r\n\r\n    & .right{\r\n        display: flex;\r\n    }\r\n    &__value {\r\n        transition: transform 0.3s;\r\n    }\r\n\r\n    &__percentage {\r\n        margin-left: 2rem;\r\n        transition: transform 0.3s;\r\n        font-size: 1.1rem;\r\n        background-color: var(--clr-red-lighten);\r\n        padding: .3rem;\r\n        border-radius: .3rem;\r\n        width: 3.2rem;\r\n        text-align: center;\r\n    }\r\n    &__delete--btn {\r\n        font-size: 22px;\r\n        background: none;\r\n        border: none;\r\n        cursor: pointer;\r\n        display: inline-block;\r\n        vertical-align: middle;\r\n        line-height: 1;\r\n        display: none;\r\n\r\n        &:focus {\r\n            outline: none;\r\n        }\r\n        &:active {\r\n            transform: translateY(2px);\r\n        }\r\n    }\r\n    &:hover &__delete--btn { display: block; } \r\n    &:hover &__value { transform: translateX(-2rem); }\r\n    &:hover &__percentage { transform: translateX(-2rem); }\r\n\r\n}\r\n.income {\r\n    & .right, \r\n    & .item__delete--btn {\r\n        color: var(--clr-green);\r\n    }\r\n}\r\n.expenses {\r\n    & .right,\r\n    & .item__delete--btn {\r\n        color: var(--clr-red);\r\n    }\r\n}\r\n\r\n.unpaid {\r\n    background-color: var(--clr-red-lighten) !important;\r\n    cursor: pointer;\r\n    color: var(--clr-red);\r\n\r\n    & .item__percentage {\r\n         box-shadow: 0 2px 6px 0 rgba(0, 0, 0, 0.1); \r\n    }\r\n\r\n    &:hover .item__description {\r\n         font-weight: 900; \r\n    }\r\n}\r\n\r\n"},"lineCount":null}},"error":null,"hash":"04271e5795559bcd80aaeeee2179aeb4","cacheData":{"env":{}}}